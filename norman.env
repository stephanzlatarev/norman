{

  "goals": [
    "show good manners",
    "react to cheesy tactics",
    "win the game"
  ],

  "body": [
    {
      "label": "Monitor",
      "code": "body/monitor",
      "config": {
        "port": 8000,
        "layout": {
          "charts": {
            "rows": [
              {
                "cols": [
                  { "chart": 3, "height": 2 },
                  { "rows": [ { "chart": 2 } ] }
                ]
              },
              {
                "cols": [
                  { "chart": 0 },
                  { "chart": 1 }
                ]
              }
            ]
          }
        },
        "charts": [
          {
            "title": "Norman",
            "type": "line",
            "ticks": { "title": "Time", "memory": [ "Norman", "time" ], "limit": 60 },
            "series": [
              { "title": "Nodes", "color": "green", "memory": [ "Norman", "nodes" ] },
              { "title": "Patterns", "color": "gold", "memory": [ "Norman", "patterns" ] },
              { "title": "Changes/sec", "color": "black", "memory": [ "Norman", "changes" ], "per": 1 }
            ]
          },
          {
            "title": "Memory patterns",
            "type": "top",
            "ticks": { "title": "Time", "memory": [ "Norman", "time" ], "limit": 60 },
            "memory": "Memory patterns", "per": 1
          },
          {
            "title": "Properties",
            "type": "line",
            "ticks": { "title": "Loop", "memory": [ "Game", "time" ], "limit": 1344 },
            "series": [
              { "title": "Minerals", "color": "blue", "memory": [ "Game", "minerals" ] },
              { "title": "Vespene", "color": "green", "memory": [ "Game", "vespene" ] }
            ]
          },
          {
            "title": "Map",
            "type": "scatter",
            "ticks": { "memory": [ "Game", "time" ] },
            "series": [
              { "title": "Fight", "color": "red", "size": 1, "memory": [ { "flag:fight": true } ], "id": "tag", "x": "x", "y": "y" },
              { "title": "Unit", "color": "green", "size": 1.5, "memory": [ { "class": "unit" } ], "id": "tag", "x": "x", "y": "y" },
              { "title": "Resource", "color": "blue", "size": 2, "memory": [ { "class": "resource" } ], "id": "tag", "x": "x", "y": "y" },
              { "title": "Building", "color": "black", "size": 3, "memory": [ { "class": "building" } ], "id": "tag", "x": "x", "y": "y" }
            ]
          }
        ]
      }
    },
    {
      "label": "StarCraft II",
      "code": "body/starcraft/local-game",
      "config": {
        "path": "C:\\games\\StarCraft II",
        "version": "Base90136",
        "realtime": false,
        "localMap": { "mapPath": "AncientCisternAIE.SC2Map" },
        "_localMap": { "mapPath": "norman-defend-nexus.SC2Map" },
        "_battlenetMapName": "Data-C",
        "playerSetup": [
          { "type": 1, "race": 3 },
          { "type": 2, "race": 4, "difficulty": 7 }
        ]
      }
    }
  ]

}
